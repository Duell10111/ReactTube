{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android/ic_launcher_foreground.png",
        "backgroundImage": "./assets/images/android/ic_launcher_background.png",
        "monochromeImage": "./assets/images/android/ic_launcher_monochrome.png"
      },
      "package": "com.duell10111.reacttube"
    },
    "ios": {
      "icon": "./assets/images/ios_icon.png",
      "bundleIdentifier": "com.duell10111.reacttube",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "isTV": false,
          "showVerboseWarnings": false,
          "tvosDeploymentTarget": "13.4",
          "removeFlipperOnAndroid": true,
          "androidTVBanner": "./assets/images/tv/tv_banner.png",
          "appleTVImages": {
            "icon": "./assets/images/tv/app_store_icon.png",
            "iconSmall": "./assets/images/tv/app_store_icon_400.png",
            "topShelf": "./assets/images/tv/app_store_icon_topshelf.png",
            "topShelf2x": "./assets/images/tv/app_store_icon_topshelf.png",
            "topShelfWide": "./assets/images/tv/app_store_icon_topshelf.png",
            "topShelfWide2x": "./assets/images/tv/app_store_icon_topshelf.png"
          }
        }
      ],
      [
        "widget-watchos-expo-plugin",
        {
          "targets": [
            {
              "type": "watch",
              "name": "watch",
              "bundleId": "com.duell10111.reacttube.watch",
              "displayName": "reacttube",
              "companionAppBundleId": "com.duell10111.reacttube",
              "sourceDir": "watch",
              "sourceFiles": ["watchApp.swift", "DBData.swift", "DBUtils.swift", "SessionSync.swift",
                "PlaylistView.swift", "MusicPlayerManager.swift", "DataController.swift", "SettingsScreen.swift",
                "MusikPlayer.swift", "Types.swift", "YoutubeAPI.swift", "YTScreens/HomeList.swift",
                "DownloadManager.swift", "LibraryView.swift", "PlaylistManager.swift"],
              "entitlementsFile": "watch.entitlements",
              "frameworks": ["SwiftUI.framework"],
              "buildConfigOverrides": {
                "WATCHOS_DEPLOYMENT_TARGET": "10.0"
              }
            }
          ]
        }
      ]
    ],
    "name": "reacttube",
    "slug": "reacttube",
    "userInterfaceStyle": "automatic"
  }
}
